<h1>User Details</h1>
<%= form_for @user, url:{action: "create"},  local: true  do |f| %>
  <%= f.label :Name %>
  <%= f.text_field :name %>

  <%= f.label :Email %>
  <%= f.email_field :email %>

  <%= f.label :"USER_TYPE_ID" %>
  <%= f.text_field :user_type_id %>

 <p>
 <%= f.submit %>
 </p>
<% end %>
